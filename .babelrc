{
    "presets": [
        [
            "env", {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "safari >= 7",
                        "not ie <= 8"
                    ]
                }
            }
        ]
    ],
    "plugins": ["transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
    },"transform-decorators"]
}
